{
	"$schema": "https://json.schemastore.org/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": false,
		"target": "esnext",
		"module": "esnext",
		"skipLibCheck": true,
		"strict": true,
		"moduleResolution": "node",
		"esModuleInterop": false,
		"typeRoots": [
			"../../node_modules/@types"
		],
		"types": [
			"node"
		],
		"rootDir": ".",
		"noEmit": true,
	},
	"include": [
		"."
	],
	"exclude": []
}